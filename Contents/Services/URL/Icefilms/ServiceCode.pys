URL_SS_LISTINGS = 'http://localhost:9292'

def NormalizeURL(url):
    return url

def MetadataObjectForURL(url):
    co = VideoClipObject(
        title   = 'foo',
        summary = 'bar'
    )

    return co

def MediaObjectsForURL(url):
    wizard_url = URL_SS_LISTINGS + '/wizard?url=%s' % String.Quote(url)
    data       = JSON.ObjectFromURL(wizard_url)

    obj = MediaObject(
      parts = [ PartObject(key = data['asset_url']) ],
    )

    return [ obj ]
