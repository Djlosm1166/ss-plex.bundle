def NormalizeURL(url):
    return url

def MetadataObjectForURL(url):
    co = VideoClipObject(
        title = 'foo',
        summary = 'bar'
        )

    return co

def MediaObjectsForURL(url):
    Log(url)
    wizard_url = "http://h.709scene.com/ss/wizard?url=%s" % String.Quote(url)
    data       = JSON.ObjectFromURL(wizard_url)

    obj = MediaObject(
      parts = [ PartObject(key = data['asset_url']) ],
    )

    return [ obj ]
